      .wrap {
        min-height: 87%;
        height: 87%;
        /* Negative indent footer by it's height */
        margin: 0 auto -60px;
      }
      
      .sidebar-nav {
        padding: 9px 0;
      }

      .bs-docs-sidenav .icon-chevron-right {
        float: right;
        margin-right: -6px;
